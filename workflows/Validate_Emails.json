      ],
      "id": "66c1e726-2764-4079-8570-f60f51e8cc73",
      "name": "Schedule Trigger1"
    },
    {
      "parameters": {
        "jsCode": "// Expecting: { emails: [...] }\nconst emails = $json.emails || [];\n\n// Strict email regex\nconst EMAIL_REGEX =\n  /^[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Za-z]{2,}$/;\n\n// Allowed characters\nconst SAFE_CHARS =\n  /^[A-Za-z0-9@._%+\\-]+$/;\n\nconst cleaned = [];\n\nfor (let raw of emails) {\n  if (!raw) continue;\n\n  let email = String(raw)\n    .trim()\n    .toLowerCase()\n    .replace(/\\s+/g, \"\"); // remove internal spaces\n\n  if (!SAFE_CHARS.test(email)) continue;\n  if (!EMAIL_REGEX.test(email)) continue;\n\n  cleaned.push(email);\n}\n\n// Deduplicate\nconst unique = [...new Set(cleaned)];\n\n// Convert → Truelist array-of-arrays format\nconst data = unique.map(e => [e]);\n\nreturn {\n  json: {\n    data\n  }\n};\n"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        576,
        -64
      ],
      "id": "1e147bdb-e11a-45f2-9b8c-0c8e8f5528b9",
      "name": "Clean and Dedupe"
    }
